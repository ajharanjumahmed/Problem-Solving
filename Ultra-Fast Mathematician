#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    string s1, s2;
    int s3[101];
    cin>>s1;
    cin>>s2;
    int len = s1.size();
    for(int i=0; i<len; i++)
    {
        if(s1[i]==s2[i]){
            s3[i] = 0;
        }
        else if(s1[i]!=s2[i]){
            s3[i] = 1;
        }
    }
    for(int i=0; i<len; i++)
    {
        cout<<s3[i];
    }
}

// https://codeforces.com/contest/61/problem/A 
