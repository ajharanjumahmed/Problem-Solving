#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string given = "codeforces";
        string s;
        cin>>s;
        int cnt = 10;
        for(int i=0; i<10; i++){
            if(s[i]==given[i])
                cnt--;
            else
                continue;
        }
        cout<<cnt<<endl;
 
    }
}

// https://codeforces.com/contest/1829/problem/A
