#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a,b;
        cin>>a>>b;
        if(a==b)
        {
            cout<<0<<endl;
        }
        else if(a>b)
        {
            int c = a-b;
            if(c%2==0)
            {
                cout<<1<<endl;
            }
            else if(c%2!=0)
            {
                cout<<2<<endl;
            }
        }
        else if(a<b)
        {
            int c = b-a;
            if(c%2==0)
            {
                cout<<2<<endl;
            }
            else if(c%2!=0)
            {
                cout<<1<<endl;
            }
        }
    }
}

// https://codeforces.com/contest/1311/problem/A
