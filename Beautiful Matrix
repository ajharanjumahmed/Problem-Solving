#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int arr[6][6];
    int row, column;
    for(int i=1; i<=5; i++)
    {
        for(int j=1; j<=5; j++)
        {
            cin>>arr[i][j];
            if(arr[i][j]==1){
                row = i;
                column = j;
            }
        }
    }
    int tot = (abs(3-row)) + (abs(3-column));
    cout<<tot<<endl;
}

// https://codeforces.com/contest/263/problem/A
