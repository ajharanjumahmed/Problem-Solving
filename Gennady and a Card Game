#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    string s1;
    cin>>s1;
    int t =5;
    while(t--)
    {
        string s2;
        cin>>s2;
        if((s1[0]==s2[0]) || (s1[0]==s2[1])){
            cout<<"YES"<<endl;
            return 0;
        }
        else if ((s1[1]==s2[0]) || (s1[1]==s2[1])){
            cout<<"YES"<<endl;
            return 0;
        }
 
    }
    cout<<"NO"<<endl;
}

// https://codeforces.com/contest/1097/problem/A
