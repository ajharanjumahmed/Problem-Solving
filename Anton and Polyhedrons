#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int n;
    cin>>n;
    int cnt =0;
    for(int i=0; i<n; i++)
    {
        string s;
        cin>>s;
        if(s[0]=='T'){
            cnt = cnt + 4;
        }
        else if(s[0]=='C'){
            cnt = cnt + 6;
        }
        else if(s[0]=='O'){
            cnt = cnt + 8;
        }
        else if(s[0]=='D'){
            cnt = cnt + 12;
        }
        else if(s[0]=='I'){
            cnt = cnt + 20;
        }
    }
    cout<<cnt<<endl;
}

// https://codeforces.com/contest/785/problem/A
